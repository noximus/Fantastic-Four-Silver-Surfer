class nav.NavigationItem extends MovieClip {	//private var mc_nav:Extensions;	private var mc_navigation:MovieClip;	function onRelease() {		if (this._name == "nav_home") {			_root.gotoAndStop("home");			//loadXML(home);		} else if (this._name == "nav_about") {			_root.gotoAndStop("about_us");		} else if (this._name == "nav_gallery") {			_root.gotoAndStop("gallery");		} else if (this._name == "nav_events") {			_root.gotoAndStop("events");		} else if (this._name == "nav_clubs") {			_root.gotoAndStop("other_clubs");		} else if (this._name == "nav_links"){			_root.gotoAndStop("links");		} else if (this._name == "nav_contact"){			_root.gotoAndStop("contact_us");		}	}}